@model IEnumerable<ProjectManagementApp.Models.AssignPerson>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>All Assign Person</h2>
<table class="table table-striped table-bordered">
    <tr>
        <th>
            @Html.DisplayName("Project Name")
        </th>
        <th>
            @Html.DisplayName("Resource Person")
        </th>
        <th>
            @Html.DisplayName("Designation")
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Projct.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.Designation.Name)
            </td>
        </tr>
    }

</table>
